{
    "family": "ec2_webapp",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "name": "webapp",
            "cpu": 100,
            "memory": 128,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ]
}
